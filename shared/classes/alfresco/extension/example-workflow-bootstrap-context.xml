<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
	<bean id="workflowBootstrapextentionCommon" parent="workflowDeployer">
		<property name="workflowDefinitions">
			<list>
				<props>
					<prop key="engineId">jbpm</prop>
					<prop key="location">alfresco/extension/workflow/leaveworkflow_processdefinition.xml</prop>
					<prop key="mimetype">text/xml</prop>
					<prop key="redeploy">true</prop>
				</props>
			</list>
		</property>
		<property name="models">
			<list>
				<value>alfresco/extension/workflow/leaveworkflow-Model.xml</value>		
			</list>
		</property>       
       <property name="labels">
			<list>
				<value>alfresco/extension/workflow/leaveworkflowcustom-messages</value>		
			</list>
		</property>		
	</bean>
</beans>